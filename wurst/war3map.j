                    endif
                endif
            endif
        endif
    else
        if ( Trig_Respawn_Trigger_Func005Func002C() ) then
            call CreateNUnitsAtLoc(1, 'nshe', Player(PLAYER_NEUTRAL_PASSIVE), udg_CreepPoint[GetUnitUserData(GetTriggerUnit())], GetUnitFacing(GetTriggerUnit()))
