// Trigger: Death Grip Loop
//
// By: Laercio
//===========================================================================
function Trig_Death_Grip_Loop_Func001Func006Func009001 takes nothing returns boolean
    return ( IsUnitGroupEmptyBJ(udg_DG_Group) == true )
endfunction

function Trig_Death_Grip_Loop_Func001Func006C takes nothing returns boolean
    if ( not ( udg_DG_Timer > 0.00 ) ) then
        return false
    endif
    if ( not ( GetUnitStateSwap(UNIT_STATE_LIFE, LoadUnitHandleBJ(5, udg_DGTargetKey, udg_HashGrip)) > 0.00 ) ) then
        return false
    endif
    return true
endfunction

function Trig_Death_Grip_Loop_Func001A takes nothing returns nothing
    set udg_DG_Target=GetEnumUnit()
    set udg_DGTargetKey=GetHandleId(udg_DG_Target)
    // Load values from the Hashtable
    set udg_DG_Timer=LoadRealBJ(2, udg_DGTargetKey, udg_HashGrip)
    // -----------------------
    if ( Trig_Death_Grip_Loop_Func001Func006C() ) then
        // Moving unit
        set udg_DGPoint1=GetUnitLoc(udg_DG_Target)
        set udg_DGPoint2=PolarProjectionBJ(udg_DGPoint1, LoadRealBJ(1, udg_DGTargetKey, udg_HashGrip), LoadRealBJ(0, udg_DGTargetKey, udg_HashGrip))
        call SetUnitPositionLoc(udg_DG_Target, udg_DGPoint2)
        call RemoveLocation(udg_DGPoint1)
        call RemoveLocation(udg_DGPoint2)
        // Decrease Timer for the movement of the unit
        call SaveRealBJ(( udg_DG_Timer - 0.04 ), 2, udg_DGTargetKey, udg_HashGrip)
    else
        // When the spell is done, clear the values...
        call DestroyEffectBJ(LoadEffectHandleBJ(3, udg_DGTargetKey, udg_HashGrip))
        call FlushChildHashtableBJ(udg_DGTargetKey, udg_HashGrip)
        call GroupRemoveUnitSimple(udg_DG_Target, udg_DG_Group)
        call SetUnitPathing(udg_DG_Target, true)
        // This actions checks if the unit group is empty and turns off the trigger if that case
        if ( (IsUnitGroupEmptyBJ(udg_DG_Group) == true) ) then // INLINED!!
            call DisableTrigger(GetTriggeringTrigger())
        else
            // Do Nothing :)
        endif
    endif
endfunction

function Trig_Death_Grip_Loop_Actions takes nothing returns nothing
    call ForGroupBJ(udg_DG_Group, function Trig_Death_Grip_Loop_Func001A)
endfunction

