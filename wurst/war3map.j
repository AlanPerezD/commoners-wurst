    call ForGroupBJ(udg_KB_Group[udg_KB_Int], function Trig_Explosive_Shot_Func009A)
    call RemoveLocation(udg_TempPoint)
    // --------------------------------------------------------------------------------------------------------
    // Checking if units are around
