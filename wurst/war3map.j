                    else
                        call CreateNUnitsAtLoc(1, 'nder', Player(PLAYER_NEUTRAL_AGGRESSIVE), udg_CreepPoint[GetUnitUserData(GetTriggerUnit())], GetUnitFacing(GetTriggerUnit()))
                    endif
                endif
            else
