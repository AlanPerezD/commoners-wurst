            endif
        endif
    endif
    if ( Trig_Respawn_Trigger_Func006C() ) then
        call SaveIntegerBJ(400, StringHashBJ("rank"), GetHandleIdBJ(GetLastCreatedUnit()), udg_RankHashtable)
