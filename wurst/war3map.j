    call AddSpecialEffectTargetUnitBJ("origin", udg_CCaster[udg_CArray], "Abilities\\Spells\\Items\\OrbCorruption\\OrbCorruptionSpecialArt.mdl")
    call DestroyEffectBJ(GetLastCreatedEffectBJ())
    // --------------- THINGS THAT YOU CAN CHANGE ---------------
    // CAcceleration = starting acceleration of the caster. Greater value = greater start speed and faster animation.
    set udg_CAcceleration[udg_CArray]=10.00
    // CAnimSpeed = starting speed of the animation of the caster.
    set udg_CAnimSpeed[udg_CArray]=100.00
    // CWait = the time the slash animation is. Change that if you have a unit with longer/shorter slash animation.
    set udg_CWait[udg_CArray]=0.40
    // CDamage = damage of the slash.
    set udg_CDamage[udg_CArray]=I2R(( ( ( GetHeroStatBJ(bj_HEROSTAT_STR, GetTriggerUnit(), true) + GetHeroStatBJ(bj_HEROSTAT_AGI, GetTriggerUnit(), true) ) * 2 ) + 40 ))
    // CAccelerationRate = the rate of the acceleration. Greater value = faster acceleration.
    set udg_CAccelerationRate=0.80
    // --------------- THINGS THAT YOU CAN CHANGE ---------------
    call RemoveLocation(udg_CCasterPosCheck)
    call RemoveLocation(udg_CTargetPosCheck)
    call RemoveLocation(udg_CCasterPos)
    call RemoveLocation(udg_CTargetPos)
endfunction

