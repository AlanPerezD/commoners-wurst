                    else
                        set udg_TempPoint=GetRectCenter(gg_rct_Respawn_3)
                    endif
                endif
            endif
            set udg_TempInt=GetForLoopIndexA()
            call ReviveHeroLoc(udg_HERO[udg_TempInt], udg_TempPoint, true)
            call SelectUnitForPlayerSingle(udg_HERO[udg_TempInt], GetOwningPlayer(GetTriggerUnit()))
