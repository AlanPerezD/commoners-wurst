    return true
endfunction

function Trig_Death_Grip_Cast_Func014001 takes nothing returns boolean
    return ( HaveSavedValue(3, bj_HASHTABLE_HANDLE, udg_DGTargetKey, udg_HashGrip) == true )
endfunction

function Trig_Death_Grip_Cast_Actions takes nothing returns nothing
    // Turn on the loop
    call EnableTrigger(gg_trg_Death_Grip_Loop)
    // Setting variables
    set udg_DG_Caster=GetTriggerUnit()
    set udg_DG_Target=GetSpellTargetUnit()
    call SetUnitPathing(udg_DG_Target, false)
    set udg_DGTargetKey=GetHandleId(udg_DG_Target)
    set udg_DGPoint1=GetUnitLoc(udg_DG_Caster)
    set udg_DGPoint2=GetUnitLoc(udg_DG_Target)
    set udg_DG_Angle=AngleBetweenPoints(udg_DGPoint2, udg_DGPoint1)
    set udg_DG_Timer=udg_DG_CastingTime
    set udg_DG_CurrentDist=( ( DistanceBetweenPoints(udg_DGPoint1, udg_DGPoint2) - udg_DG_Collision ) * ( 0.04 / udg_DG_Timer ) )
    // Check if Unit is already with Special Effect, if not, create the special effect
    if ( (HaveSavedValue(3, bj_HASHTABLE_HANDLE, udg_DGTargetKey, udg_HashGrip) == true) ) then // INLINED!!
        // Do Nothing
    else
        call AddSpecialEffectTargetUnitBJ(udg_DG_Attach_Target, udg_DG_Target, udg_DG_TargetEffect)
    endif
    // Removing Leaks
    call RemoveLocation(udg_DGPoint1)
    call RemoveLocation(udg_DGPoint2)
    // Saving values in the Hashtable
    call SaveRealBJ(udg_DG_Angle, 0, udg_DGTargetKey, udg_HashGrip)
    call SaveRealBJ(udg_DG_CurrentDist, 1, udg_DGTargetKey, udg_HashGrip)
    call SaveRealBJ(udg_DG_Timer, 2, udg_DGTargetKey, udg_HashGrip)
    call SaveEffectHandleBJ(GetLastCreatedEffectBJ(), 3, udg_DGTargetKey, udg_HashGrip)
    call SaveUnitHandleBJ(udg_DG_Caster, 5, udg_DGTargetKey, udg_HashGrip)
    // Add the target to the unit group
    call GroupAddUnitSimple(udg_DG_Target, udg_DG_Group)
endfunction

