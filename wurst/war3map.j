    call ShowUnitHide(GetEnumUnit())
endfunction

function Trig_Rotate_Structure_Func009A takes nothing returns nothing
    call ShowUnitShow(GetEnumUnit())
endfunction

function Trig_Rotate_Structure_Actions takes nothing returns nothing
    local unit u= GetTriggerUnit()
    call SetUnitFacing(u, GetUnitFacing(u) + 45)
    call TriggerSleepAction(1.00)
    set udg_triggerUnitLoc=GetUnitLoc(GetTriggerUnit())
    set udg_Group=GetUnitsInRangeOfLocMatching(1024.00, udg_triggerUnitLoc, Condition(function Trig_Rotate_Structure_Func005002003))
    call GroupRemoveUnitSimple(GetTriggerUnit(), udg_Group)
    call ForGroupBJ(udg_Group, function Trig_Rotate_Structure_Func007A)
    call SetUnitPosition(u, GetUnitX(u), GetUnitY(u))
    call ForGroupBJ(udg_Group, function Trig_Rotate_Structure_Func009A)
    call DestroyGroup(udg_Group)
    call RemoveLocation(udg_triggerUnitLoc)
    set u=null
endfunction

