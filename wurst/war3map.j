endfunction

function Trig_Gravity_Well_Loop_Func001A takes nothing returns nothing
    set udg_Temp_Unit_1=GetEnumUnit()
    set udg_Temp_Unit_2=LoadUnitHandleBJ(StringHashBJ("beacon"), GetHandleIdBJ(GetEnumUnit()), udg_GW_SpellHash)
    set udg_Temp_Real_1=LoadRealBJ(StringHashBJ("distance"), GetHandleIdBJ(GetEnumUnit()), udg_GW_SpellHash)
    // --------------------
    // If beacon isn't there or unit has escaped the dragging distance, remove from group
    // --------------------
    if ( Trig_Gravity_Well_Loop_Func001Func007C() ) then
        if ( Trig_Gravity_Well_Loop_Func001Func007Func005C() ) then
            set udg_Temp_Loc_1=GetUnitLoc(udg_Temp_Unit_1)
            // --------------------
            set udg_Temp_Real_3=LoadRealBJ(StringHashBJ("angle"), GetHandleIdBJ(GetEnumUnit()), udg_GW_SpellHash)
            // --------------------
            set udg_Temp_Real_4=GetLocationX(udg_Temp_Loc_1)
            set udg_Temp_Real_5=GetLocationY(udg_Temp_Loc_1)
            // --------------------
            set udg_Temp_Real_4=( udg_Temp_Real_4 + ( udg_GW_DragSpeed * CosBJ(udg_Temp_Real_3) ) )
            set udg_Temp_Real_5=( udg_Temp_Real_5 + ( udg_GW_DragSpeed * SinBJ(udg_Temp_Real_3) ) )
            // --------------------
            set udg_Temp_Loc_2=Location(udg_Temp_Real_4, udg_Temp_Real_5)
            // --------------------
            if ( Trig_Gravity_Well_Loop_Func001Func007Func005Func013C() ) then
                call SetUnitX(udg_Temp_Unit_1, udg_Temp_Real_4)
                call SetUnitY(udg_Temp_Unit_1, udg_Temp_Real_5)
            else
            endif
            // --------------------
            call RemoveLocation(udg_Temp_Loc_1)
            call RemoveLocation(udg_Temp_Loc_2)
            // --------------------
        else
        endif
        set udg_Temp_Loc_1=GetUnitLoc(udg_Temp_Unit_1)
        set udg_Temp_Loc_2=LoadLocationHandleBJ(StringHashBJ("center"), GetHandleIdBJ(GetEnumUnit()), udg_GW_SpellHash)
        call SaveRealBJ(AngleBetweenPoints(udg_Temp_Loc_1, udg_Temp_Loc_2), StringHashBJ("angle"), GetHandleIdBJ(GetEnumUnit()), udg_GW_SpellHash)
        call SaveRealBJ(DistanceBetweenPoints(udg_Temp_Loc_2, udg_Temp_Loc_1), StringHashBJ("distance"), GetHandleIdBJ(GetEnumUnit()), udg_GW_SpellHash)
        // --------------------
        call RemoveLocation(udg_Temp_Loc_1)
    else
        set udg_Temp_Loc_2=LoadLocationHandleBJ(StringHashBJ("center"), GetHandleIdBJ(GetEnumUnit()), udg_GW_SpellHash)
        call RemoveLocation(udg_Temp_Loc_2)
        // --------------------
        call FlushChildHashtableBJ(GetHandleIdBJ(GetEnumUnit()), udg_GW_SpellHash)
        call GroupRemoveUnitSimple(udg_Temp_Unit_1, udg_GW_SpellGroup)
        // --------------------
        if ( Trig_Gravity_Well_Loop_Func001Func007Func012C() ) then
            call DisableTrigger(GetTriggeringTrigger())
        else
        endif
    endif
endfunction

function Trig_Gravity_Well_Loop_Actions takes nothing returns nothing
    call ForGroupBJ(udg_GW_SpellGroup, function Trig_Gravity_Well_Loop_Func001A)
endfunction

