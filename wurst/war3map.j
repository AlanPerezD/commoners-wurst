function Trig_Dark_of_the_Night_Actions takes nothing returns nothing
    // Transition
    set udg_IsNight=true
    call EnableTrigger(gg_trg_Moladune_Entrance_1)
    call SetPlayerAbilityAvailableBJ(true, 'ANdp', Player(PLAYER_NEUTRAL_AGGRESSIVE))
    call SetDoodadAnimationRectBJ("stand", 'LOtz', GetPlayableMapRect())
    call SetDoodadAnimationRectBJ("stand", 'LObz', GetPlayableMapRect())
    call SetDoodadAnimationRectBJ("stand", 'LOlp', GetPlayableMapRect())
    call SetDoodadAnimationRectBJ("stand", 'NOfp', GetPlayableMapRect())
    set udg_TempPoint=GetRectCenter(gg_rct_Vampires)
    if ( Trig_Dark_of_the_Night_Func010C() ) then
        call CreateNUnitsAtLoc(3, 'ugar', Player(10), udg_TempPoint, GetRandomDirectionDeg())
        call CreateNUnitsAtLoc(3, 'ndqt', Player(10), udg_TempPoint, GetRandomDirectionDeg())
    else
        call CreateNUnitsAtLoc(1, 'ndqt', Player(10), udg_TempPoint, GetRandomDirectionDeg())
    endif
    call AddSpecialEffectLocBJ(udg_TempPoint, "Abilities\\Spells\\Undead\\Darksummoning\\DarkSummonTarget.mdl")
    call DestroyEffectBJ(GetLastCreatedEffectBJ())
    set udg_TempGroup=GetUnitsOfPlayerAndTypeId(Player(11), 'nsqe')
    if ( Trig_Dark_of_the_Night_Func014C() ) then
        call CreateNUnitsAtLoc(1, 'nsqe', Player(11), udg_PointPirates[1], GetRandomDirectionDeg())
        call SaveIntegerBJ(0, StringHashBJ("rank"), GetHandleIdBJ(GetLastCreatedUnit()), udg_RankHashtable)
        call CreateNUnitsAtLoc(1, 'nsqe', Player(11), udg_PointPirates[2], GetRandomDirectionDeg())
        call SaveIntegerBJ(0, StringHashBJ("rank"), GetHandleIdBJ(GetLastCreatedUnit()), udg_RankHashtable)
        call CreateNUnitsAtLoc(1, 'nsqe', Player(11), udg_PointPirates[3], GetRandomDirectionDeg())
        call SaveIntegerBJ(0, StringHashBJ("rank"), GetHandleIdBJ(GetLastCreatedUnit()), udg_RankHashtable)
    else
    endif
    call DestroyGroup(udg_TempGroup)
    // Daily Income
    call TriggerExecute(gg_trg_Daily_Income)
    set udg_TempGroup=GetUnitsOfPlayerAll(Player(8))
    call ForGroupBJ(udg_TempGroup, function Trig_Dark_of_the_Night_Func019A)
    call DestroyGroup(udg_TempGroup)
    // Moladune Mines Reset
    call TriggerExecute(gg_trg_Moladune_Mines_Reset)
    // Eternal Grove Reset
    call TriggerExecute(gg_trg_Eternal_Grove_Reset)
    // Seafarer's Rest Reset
    call TriggerExecute(gg_trg_Seafarers_Rest_Reset)
    // Blood Moon
    call StopMusicBJ(false)
    if ( Trig_Dark_of_the_Night_Func029C() ) then
        if ( Trig_Dark_of_the_Night_Func029Func004C() ) then
            call TriggerExecute(gg_trg_Blood_Moon)
            call PlayMusicBJ(gg_snd_BloodMoon)
        else
            call PlayMusicBJ(gg_snd_DemonInvasion)
            set udg_TempPoint=GetRectCenter(gg_rct_Demon_Gate)
            call PingMinimapLocForForceEx(GetPlayersAll(), udg_TempPoint, 3.00, bj_MINIMAPPINGSTYLE_SIMPLE, 100.00, 25.00, 25.00)
            call RemoveLocation(udg_TempPoint)
            call DisplayTextToForce(GetPlayersAll(), "TRIGSTR_8399")
        endif
        call SetTerrainFogExBJ(0, 0.00, 6000.00, 1.00, 40.00, 0.00, 0.00)
        if ( Trig_Dark_of_the_Night_Func029Func006C() ) then
            call SetUnitInvulnerable(udg_DemonGate, false)
            call SetUnitOwner(udg_DemonGate, Player(10), false)
            call SetUnitAnimation(udg_DemonGate, "work")
            call CreateNUnitsAtLoc(1, 'nerw', Player(11), udg_PointDemonLordSpawn, bj_UNIT_FACING)
        else
        endif
    else
        call SetTerrainFogExBJ(0, 0.00, 6000.00, 1.00, 0.00, 0.00, 20.00)
        call PlayMusicBJ(gg_snd_SadMystery)
    endif
    // Weather
    if ( Trig_Dark_of_the_Night_Func031C() ) then
        set udg_TempInt=GetRandomInt(0, 3)
        if ( Trig_Dark_of_the_Night_Func031Func002C() ) then
            if ( Trig_Dark_of_the_Night_Func031Func002Func007C() ) then
                call EnableWeatherEffect(udg_Weather_Effects[udg_Weather_Current], false)
                call EnableWeatherEffect(udg_Weather_Effects[( udg_Weather_Current + 3 )], false)
                call EnableWeatherEffect(udg_Weather_Effects[( udg_Weather_Current + 6 )], false)
                call EnableWeatherEffect(udg_Weather_Effects[( udg_Weather_Current + 9 )], false)
                call EnableWeatherEffect(udg_Weather_Effects[( udg_Weather_Current + 12 )], false)
            else
            endif
            if ( Trig_Dark_of_the_Night_Func031Func002Func008C() ) then
                call EnableWeatherEffect(udg_Weather_Effects[udg_TempInt], true)
                call EnableWeatherEffect(udg_Weather_Effects[( udg_TempInt + 3 )], true)
                call EnableWeatherEffect(udg_Weather_Effects[( udg_TempInt + 6 )], true)
                call EnableWeatherEffect(udg_Weather_Effects[( udg_TempInt + 9 )], true)
                call EnableWeatherEffect(udg_Weather_Effects[( udg_TempInt + 12 )], true)
                if ( Trig_Dark_of_the_Night_Func031Func002Func008Func007C() ) then
                    call PlaySoundBJ(gg_snd_RollingThunder)
                else
                endif
            else
            endif
            call EnableWeatherEffect(udg_Weather_Effects[2], false)
            call EnableWeatherEffect(udg_Weather_Effects[5], false)
            call EnableWeatherEffect(udg_Weather_Effects[8], false)
            call EnableWeatherEffect(udg_Weather_Effects[11], false)
            call EnableWeatherEffect(udg_Weather_Effects[14], false)
            set udg_Weather_Current=udg_TempInt
        else
            call EnableWeatherEffect(udg_Weather_Effects[2], false)
            call EnableWeatherEffect(udg_Weather_Effects[5], false)
            call EnableWeatherEffect(udg_Weather_Effects[8], false)
            call EnableWeatherEffect(udg_Weather_Effects[11], false)
            call EnableWeatherEffect(udg_Weather_Effects[14], false)
        endif
    else
    endif
endfunction

