                call SetUnitFlyHeightBJ(udg_TK_Unit[udg_TK_Integers[3]], ( ( SinBJ(udg_TK_Interval[udg_TK_Integers[3]]) * udg_TK_MaxHeight[udg_TK_Integers[3]] ) + GetUnitDefaultFlyHeight(udg_TK_Unit[udg_TK_Integers[3]]) ), 0.00)
