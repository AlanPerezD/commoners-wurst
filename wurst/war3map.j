function Trig_General_Dialog_Actions takes nothing returns nothing
    set udg_TempInt=GetConvertedPlayerId(GetTriggerPlayer())
    if ( Trig_General_Dialog_Func002C() ) then
        set udg_Diplomacy_Ally[udg_TempInt]=false
        set udg_Diplomacy_Truce[udg_TempInt]=false
        set udg_Diplomacy_DeclareWar[udg_TempInt]=false
        set udg_Diplomacy_Denounce[udg_TempInt]=false
        set udg_Diplomacy_Tribute[udg_TempInt]=false
    else
        set udg_TempPlayer=GetTriggerPlayer()
        // Diplomacy Dialog
        // Broker Peace
        if ( Trig_General_Dialog_Func002Func009C() ) then
            if ( Trig_General_Dialog_Func002Func009Func003C() ) then
                call ForForce(udg_NeutralPlayers, function Trig_General_Dialog_Func002Func009Func003Func004A)
                call ForForce(udg_BanditPlayers, function Trig_General_Dialog_Func002Func009Func003Func005A)
                call DisplayTextToForce(GetPlayersAll(), ( ( udg_PlayerColor[udg_TempInt] + GetPlayerName(GetTriggerPlayer()) ) + "|r has brokered peace between all non-bandit factions. Affected players are neutral, reputation is reset, and participants can't attack each other for 2 minutes." ))
                set udg_PeaceBrokered=true
                call TriggerExecute(gg_trg_Peace_Out)
            else
                set udg_TempForce=GetForceOfPlayer(udg_TempPlayer)
                call DestroyForce(udg_TempForce)
            endif
        else
            // Make Alliance
            if ( Trig_General_Dialog_Func002Func009Func002C() ) then
                set udg_Diplomacy_Ally[udg_TempInt]=true
                call DialogClearBJ(udg_Selection_Dialog[udg_TempInt])
                call DialogSetMessageBJ(udg_Selection_Dialog[udg_TempInt], "TRIGSTR_4951")
                call DialogAddButtonBJ(udg_Selection_Dialog[udg_TempInt], "TRIGSTR_3614")
                set udg_Button_Cancel[udg_TempInt]=GetLastCreatedButtonBJ()
                set bj_forLoopAIndex=1
                set bj_forLoopAIndexEnd=16
                loop
                    exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
                    if ( Trig_General_Dialog_Func002Func009Func002Func007Func001C() ) then
                        call DialogAddButtonBJ(udg_Selection_Dialog[udg_TempInt], ( udg_PlayerColor[GetForLoopIndexA()] + ( GetPlayerName(ConvertedPlayer(GetForLoopIndexA())) + "|r" ) ))
                        set udg_Button_Player[GetForLoopIndexA()]=GetLastCreatedButtonBJ()
                    else
                    endif
                    set bj_forLoopAIndex=bj_forLoopAIndex + 1
                endloop
                call DialogDisplayBJ(true, udg_Selection_Dialog[udg_TempInt], udg_TempPlayer)
            else
                // Make Truce
                if ( Trig_General_Dialog_Func002Func009Func002Func009C() ) then
                    set udg_Diplomacy_Truce[udg_TempInt]=true
                    call DialogClearBJ(udg_Selection_Dialog[udg_TempInt])
                    call DialogSetMessageBJ(udg_Selection_Dialog[udg_TempInt], "TRIGSTR_5128")
                    call DialogAddButtonBJ(udg_Selection_Dialog[udg_TempInt], "TRIGSTR_3614")
                    set udg_Button_Cancel[udg_TempInt]=GetLastCreatedButtonBJ()
                    set bj_forLoopAIndex=1
                    set bj_forLoopAIndexEnd=16
                    loop
                        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
                        if ( Trig_General_Dialog_Func002Func009Func002Func009Func007Func001C() ) then
                            call DialogAddButtonBJ(udg_Selection_Dialog[udg_TempInt], ( udg_PlayerColor[GetForLoopIndexA()] + ( GetPlayerName(ConvertedPlayer(GetForLoopIndexA())) + "|r" ) ))
                            set udg_Button_Player[GetForLoopIndexA()]=GetLastCreatedButtonBJ()
                        else
                        endif
                        set bj_forLoopAIndex=bj_forLoopAIndex + 1
                    endloop
                    call DialogDisplayBJ(true, udg_Selection_Dialog[udg_TempInt], udg_TempPlayer)
                else
                    // Declare War
                    if ( Trig_General_Dialog_Func002Func009Func002Func009Func009C() ) then
                        set udg_Diplomacy_DeclareWar[udg_TempInt]=true
                        call DialogClearBJ(udg_Selection_Dialog[udg_TempInt])
                        call DialogSetMessageBJ(udg_Selection_Dialog[udg_TempInt], "TRIGSTR_4960")
                        call DialogAddButtonBJ(udg_Selection_Dialog[udg_TempInt], "TRIGSTR_3614")
                        set udg_Button_Cancel[udg_TempInt]=GetLastCreatedButtonBJ()
                        set bj_forLoopAIndex=1
                        set bj_forLoopAIndexEnd=16
                        loop
                            exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
                            if ( Trig_General_Dialog_Func002Func009Func002Func009Func009Func008Func001C() ) then
                                call DialogAddButtonBJ(udg_Selection_Dialog[udg_TempInt], ( udg_PlayerColor[GetForLoopIndexA()] + ( GetPlayerName(ConvertedPlayer(GetForLoopIndexA())) + "|r" ) ))
                                set udg_Button_Player[GetForLoopIndexA()]=GetLastCreatedButtonBJ()
                            else
                            endif
                            set bj_forLoopAIndex=bj_forLoopAIndex + 1
                        endloop
                        call DialogDisplayBJ(true, udg_Selection_Dialog[udg_TempInt], udg_TempPlayer)
                    else
                        // Denounce
                        if ( Trig_General_Dialog_Func002Func009Func002Func009Func009Func002C() ) then
                            set udg_Diplomacy_Denounce[udg_TempInt]=true
                            call DialogClearBJ(udg_Selection_Dialog[udg_TempInt])
                            call DialogSetMessageBJ(udg_Selection_Dialog[udg_TempInt], "TRIGSTR_4979")
                            call DialogAddButtonBJ(udg_Selection_Dialog[udg_TempInt], "TRIGSTR_3614")
                            set udg_Button_Cancel[udg_TempInt]=GetLastCreatedButtonBJ()
                            set bj_forLoopAIndex=1
                            set bj_forLoopAIndexEnd=16
                            loop
                                exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
                                if ( Trig_General_Dialog_Func002Func009Func002Func009Func009Func002Func008Func001C() ) then
                                    call DialogAddButtonBJ(udg_Selection_Dialog[udg_TempInt], ( udg_PlayerColor[GetForLoopIndexA()] + ( GetPlayerName(ConvertedPlayer(GetForLoopIndexA())) + "|r" ) ))
                                    set udg_Button_Player[GetForLoopIndexA()]=GetLastCreatedButtonBJ()
                                else
                                endif
                                set bj_forLoopAIndex=bj_forLoopAIndex + 1
                            endloop
                            call DialogDisplayBJ(true, udg_Selection_Dialog[udg_TempInt], udg_TempPlayer)
                        else
                            // Quest Dialog
                            if ( Trig_General_Dialog_Func002Func009Func002Func009Func009Func002Func002C() ) then
                                if ( Trig_General_Dialog_Func002Func009Func002Func009Func009Func002Func002Func003C() ) then
                                    set udg_QuestActiveMoladune=true
                                    call CreateQuestBJ(bj_QUESTTYPE_OPT_DISCOVERED, "TRIGSTR_4822", "TRIGSTR_4823", "ReplaceableTextures\\CommandButtons\\BTNGoldMine.blp")
                                    set udg_QuestMoladune=GetLastCreatedQuestBJ()
                                    set udg_TempForce=GetForceOfPlayer(udg_TempPlayer)
                                    call QuestMessageBJ(udg_TempForce, bj_QUESTMESSAGE_DISCOVERED, "TRIGSTR_4824")
                                    call FlashQuestDialogButtonBJ()
                                    call DestroyForce(udg_TempForce)
                                    set udg_TempPoint=GetUnitLoc(gg_unit_ogrk_0256)
                                    call PlaySoundAtPointBJ(gg_snd_WhatMoladune, 75.00, udg_TempPoint, 0)
                                    call CreateTextTagLocBJ("TRIGSTR_398", udg_TempPoint, 0, 10.00, 100, 100.00, 100.00, 0)
                                    set udg_TempFloatText=GetLastCreatedTextTag()
                                    call SetTextTagVelocityBJ(udg_TempFloatText, 32.00, 90)
                                    call SetTextTagPermanentBJ(udg_TempFloatText, false)
                                    call SetTextTagLifespanBJ(udg_TempFloatText, 4.00)
                                    call SetTextTagFadepointBJ(udg_TempFloatText, 3.00)
