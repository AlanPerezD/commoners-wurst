function Trig_Light_of_the_Day_Actions takes nothing returns nothing
    // Transition
    set udg_IsNight=false
    call SetPlayerAbilityAvailableBJ(false, 'ANdp', Player(PLAYER_NEUTRAL_AGGRESSIVE))
    call ResetTerrainFogBJ()
    call SetDoodadAnimationRectBJ("death", 'LOtz', GetPlayableMapRect())
    call SetDoodadAnimationRectBJ("death", 'LOlp', GetPlayableMapRect())
    call SetDoodadAnimationRectBJ("death", 'LObz', GetPlayableMapRect())
    call SetDoodadAnimationRectBJ("death", 'NOfp', GetPlayableMapRect())
    call CreateNUnitsAtLoc(1, 'nqb4', Player(10), udg_PointPatrol[7], bj_UNIT_FACING)
    call UnitApplyTimedLifeBJ(360.00, 'BTLF', GetLastCreatedUnit())
    call DisableTrigger(gg_trg_Moladune_Entrance_1)
    call TriggerExecute(gg_trg_Clear_Dungeons)
    if ( Trig_Light_of_the_Day_Func013C() ) then
        call SetUnitInvulnerable(udg_DemonGate, true)
        call SetUnitOwner(udg_DemonGate, Player(PLAYER_NEUTRAL_PASSIVE), false)
        call SetUnitAnimation(udg_DemonGate, "stand alternate")
    else
    endif
    set udg_TempGroup=GetUnitsOfPlayerAll(Player(8))
    call ForGroupBJ(udg_TempGroup, function Trig_Light_of_the_Day_Func015A)
    call DestroyGroup(udg_TempGroup)
    // Ashford Respawn
    call TriggerExecute(gg_trg_Ashford_Respawn)
    // Droknar Respawn
    call TriggerExecute(gg_trg_Droknar_Respawn)
    // Cavalon Respawn
    call TriggerExecute(gg_trg_Cavalon_Respawn)
    // Small Town Respawn
    call TriggerExecute(gg_trg_Small_Town_Respawn)
    // Weather
    call StopMusicBJ(false)
    if ( Trig_Light_of_the_Day_Func027C() ) then
        if ( Trig_Light_of_the_Day_Func027Func001C() ) then
            call TriggerExecute(gg_trg_Natures_Wrath)
            call PlayMusicBJ(gg_snd_NaturesWrath)
        else
            call TriggerExecute(gg_trg_Verdant_Rains)
            call PlayMusicBJ(gg_snd_VerdantRain)
        endif
        set udg_TempInt=1
    else
        set udg_TempInt=GetRandomInt(0, 3)
        call PlayMusicBJ(gg_snd_OrcTheme)
    endif
    if ( Trig_Light_of_the_Day_Func028C() ) then
        if ( Trig_Light_of_the_Day_Func028Func001C() ) then
            if ( Trig_Light_of_the_Day_Func028Func001Func002C() ) then
                call EnableWeatherEffect(udg_Weather_Effects[udg_Weather_Current], false)
                call EnableWeatherEffect(udg_Weather_Effects[( udg_Weather_Current + 3 )], false)
                call EnableWeatherEffect(udg_Weather_Effects[( udg_Weather_Current + 6 )], false)
                call EnableWeatherEffect(udg_Weather_Effects[( udg_Weather_Current + 9 )], false)
                call EnableWeatherEffect(udg_Weather_Effects[( udg_Weather_Current + 12 )], false)
            else
                if ( Trig_Light_of_the_Day_Func028Func001Func002Func001C() ) then
                    if ( Trig_Light_of_the_Day_Func028Func001Func002Func001Func001C() ) then
                        call SetTerrainFogExBJ(0, 0.00, 8000.00, 1.00, 0.00, 100.00, 0.00)
                    else
                        call SetTerrainFogExBJ(0, 0.00, 8000.00, 1.00, 0.00, 0.00, 100.00)
                    endif
                else
                endif
            endif
            if ( Trig_Light_of_the_Day_Func028Func001Func003C() ) then
                call EnableWeatherEffect(udg_Weather_Effects[udg_TempInt], true)
                call EnableWeatherEffect(udg_Weather_Effects[( udg_TempInt + 3 )], true)
                call EnableWeatherEffect(udg_Weather_Effects[( udg_TempInt + 6 )], true)
                call EnableWeatherEffect(udg_Weather_Effects[( udg_TempInt + 9 )], true)
                call EnableWeatherEffect(udg_Weather_Effects[( udg_TempInt + 12 )], true)
                if ( Trig_Light_of_the_Day_Func028Func001Func003Func007C() ) then
                    call PlaySoundBJ(gg_snd_RollingThunder)
                    if ( Trig_Light_of_the_Day_Func028Func001Func003Func007Func002C() ) then
                        call SetTerrainFogExBJ(0, 0.00, 8000.00, 1.00, 0.00, 100.00, 0.00)
                    else
                        call SetTerrainFogExBJ(0, 0.00, 8000.00, 1.00, 0.00, 0.00, 100.00)
                    endif
                else
                endif
            else
            endif
        else
        endif
    else
    endif
    set udg_Weather_Current=udg_TempInt
endfunction

