function Trig_Charge_Loop_Actions takes nothing returns nothing
    set udg_CLoop=1
    loop
        exitwhen udg_CLoop > udg_ID2
        set udg_CArray=udg_ID[udg_CLoop]
        set udg_CCasterPos=GetUnitLoc(udg_CCaster[udg_CArray])
        set udg_CTargetPos=GetUnitLoc(udg_CTarget[udg_CArray])
        set udg_CAngle[udg_CArray]=AngleBetweenPoints(udg_CCasterPos, udg_CTargetPos)
        call SetUnitFacingTimed(udg_CCaster[udg_CArray], udg_CAngle[udg_CArray], 0)
        set udg_CDistance[udg_CArray]=DistanceBetweenPoints(udg_CCasterPos, udg_CTargetPos)
        call RemoveLocation(udg_CTargetPos)
        if ( Trig_Charge_Loop_Func001Func008C() ) then
            if ( Trig_Charge_Loop_Func001Func008Func001C() ) then
                call CreateNUnitsAtLoc(1, 'oeye', GetOwningPlayer(udg_CCaster[udg_CArray]), udg_CCasterPos, bj_UNIT_FACING)
                call UnitApplyTimedLifeBJ(1.00, 'BTLF', GetLastCreatedUnit())
                call IssueTargetOrderBJ(GetLastCreatedUnit(), "thunderbolt", udg_CTarget[udg_CArray])
                call SetUnitTimeScalePercent(udg_CCaster[udg_CArray], 200.00)
                call ResetUnitAnimation(udg_CCaster[udg_CArray])
                call SetUnitAnimation(udg_CCaster[udg_CArray], "Attack Slam")
                set udg_CEndOfCharge[udg_CArray]=true
            else
            endif
            if ( Trig_Charge_Loop_Func001Func008Func002C() ) then
                call UnitDamageTargetBJ(udg_CCaster[udg_CArray], udg_CTarget[udg_CArray], udg_CDamage[udg_CArray], ATTACK_TYPE_HERO, DAMAGE_TYPE_NORMAL)
                call AddSpecialEffectTargetUnitBJ("chest", udg_CTarget[udg_CArray], "Abilities\\Spells\\Other\\Stampede\\StampedeMissileDeath.mdl")
                call DestroyEffectBJ(GetLastCreatedEffectBJ())
                call IssueTargetOrderBJ(udg_CCaster[udg_CArray], "attack", udg_CTarget[udg_CArray])
                call SetUnitTimeScalePercent(udg_CCaster[udg_CArray], 100.00)
                call SetUnitPathing(udg_CCaster[udg_CArray], true)
                set udg_ID[udg_CLoop]=udg_ID[udg_ID2]
                set udg_ID[udg_ID2]=udg_CArray
                set udg_ID2=( udg_ID2 - 1 )
                set udg_CLoop=( udg_CLoop - 1 )
                if ( Trig_Charge_Loop_Func001Func008Func002Func013C() ) then
                    call DisableTrigger(GetTriggeringTrigger())
                else
                endif
            else
                set udg_CWait[udg_CArray]=( udg_CWait[udg_CArray] - 0.03 )
                call IssueImmediateOrderBJ(udg_CCaster[udg_CArray], "stop")
            endif
        else
            call IssueImmediateOrderBJ(udg_CCaster[udg_CArray], "stop")
            if ( Trig_Charge_Loop_Func001Func008Func004C() ) then
                call SetUnitAnimationByIndex(udg_CCaster[udg_CArray], 6)
            else
                if ( Trig_Charge_Loop_Func001Func008Func004Func001C() ) then
                    call SetUnitAnimationByIndex(udg_CCaster[udg_CArray], 4)
                else
                    if ( Trig_Charge_Loop_Func001Func008Func004Func001Func001C() ) then
                        call SetUnitAnimationByIndex(udg_CCaster[udg_CArray], 2)
                    else
                        if ( Trig_Charge_Loop_Func001Func008Func004Func001Func001Func002C() ) then
                            call SetUnitAnimationByIndex(udg_CCaster[udg_CArray], 9)
                        else
                            call SetUnitAnimationByIndex(udg_CCaster[udg_CArray], 14)
                        endif
                    endif
                endif
            endif
            set udg_CAcceleration[udg_CArray]=( udg_CAcceleration[udg_CArray] + udg_CAccelerationRate )
            set udg_CMovePoint=PolarProjectionBJ(udg_CCasterPos, udg_CAcceleration[udg_CArray], udg_CAngle[udg_CArray])
            call RemoveLocation(udg_CCasterPos)
            if ( Trig_Charge_Loop_Func001Func008Func008C() ) then
                set udg_CAnimSpeed[udg_CArray]=( udg_CAnimSpeed[udg_CArray] + ( udg_CAcceleration[udg_CArray] / 3.00 ) )
                call SetUnitTimeScalePercent(udg_CCaster[udg_CArray], udg_CAnimSpeed[udg_CArray])
            else
            endif
            if ( Trig_Charge_Loop_Func001Func008Func009C() ) then
                call DestroyForce(udg_CPlayaGroup[udg_CArray])
                call SetUnitTimeScalePercent(udg_CCaster[udg_CArray], 100.00)
                call SetUnitPathing(udg_CCaster[udg_CArray], true)
                call IssueImmediateOrderBJ(udg_CCaster[udg_CArray], "stop")
                call ResetUnitAnimation(udg_CCaster[udg_CArray])
                set udg_ID[udg_CLoop]=udg_ID[udg_ID2]
                set udg_ID[udg_ID2]=udg_CArray
                set udg_ID2=( udg_ID2 - 1 )
                set udg_CLoop=( udg_CLoop - 1 )
                if ( Trig_Charge_Loop_Func001Func008Func009Func010C() ) then
                    call DisableTrigger(GetTriggeringTrigger())
                else
                endif
            else
                call SetUnitX(udg_CCaster[udg_CArray], GetLocationX(udg_CMovePoint))
                call SetUnitY(udg_CCaster[udg_CArray], GetLocationY(udg_CMovePoint))
                call RemoveLocation(udg_CMovePoint)
            endif
        endif
        set udg_CLoop=udg_CLoop + 1
    endloop
endfunction

