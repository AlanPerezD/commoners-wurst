    set udg_CCaster[udg_CArray]=udg_CCasterCheck
    set udg_CTarget[udg_CArray]=udg_CTargetCheck
    set udg_CEndOfCharge[udg_CArray]=false
    set udg_CPlayaGroup[udg_CArray]=GetPlayersByMapControl(GetPlayerController(GetOwningPlayer(udg_CCaster[udg_CArray])))
